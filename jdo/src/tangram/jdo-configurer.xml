<?xml version="1.0" encoding="utf-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<!-- velocity activation for app engine repository -->

	<bean id="velocityRepositoryViewResolver" class="org.tangram.jdo.view.ModelAwareRepositoryViewResolver">
		<property name="delegate" ref="plainRepositoryVelocityViewResolver" />
		<property name="order" value="10000"/>
		<property name="activateCaching" value="true" />
	</bean>
 
</beans>
